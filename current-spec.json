{
  "name": "qylon-app",
  "services": [
    {
      "name": "qylon",
      "github": {
        "repo": "KD-Squares/qylon",
        "branch": "main",
        "deploy_on_push": true
      },
      "build_command": "npm run build",
      "source_dir": "/",
      "environment_slug": "node-js",
      "instance_size_slug": "apps-s-1vcpu-1gb",
      "instance_count": 2,
      "http_port": 8080
    }
  ],
  "region": "fra",
  "alerts": [
    {
      "rule": "DEPLOYMENT_FAILED"
    },
    {
      "rule": "DOMAIN_FAILED"
    }
  ],
  "ingress": {
    "rules": [
      {
        "match": {
          "path": {
            "prefix": "/"
          },
          "authority": {}
        },
        "component": {
          "name": "qylon"
        }
      }
    ]
  },
  "features": [
    "buildpack-stack=ubuntu-22"
  ]
}
