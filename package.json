{
  "name": "qylon",
  "version": "1.0.0",
  "description": "AI Automation Platform - Transform manual business processes into intelligent, self-running systems",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:api-gateway\" \"npm run dev:security\" \"npm run dev:meeting-intelligence\" \"npm run dev:workflow-automation\"",
    "dev:api-gateway": "cd services/api-gateway && npm run dev",
    "dev:security": "cd services/security && npm run dev",
    "dev:meeting-intelligence": "cd services/meeting-intelligence && npm run dev",
    "dev:workflow-automation": "cd services/workflow-automation && npm run dev",
    "build": "npm run build:api-gateway && npm run build:security && npm run build:meeting-intelligence && npm run build:workflow-automation",
    "build:api-gateway": "cd services/api-gateway && npm run build",
    "build:security": "cd services/security && npm run build",
    "build:meeting-intelligence": "cd services/meeting-intelligence && npm run build",
    "build:workflow-automation": "cd services/workflow-automation && npm run build",
    "install:all": "npm install && npm run install:services",
    "install:services": "cd services/api-gateway && npm install && cd ../security && npm install && cd ../meeting-intelligence && npm install && cd ../workflow-automation && npm install",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --testPathPattern=tests/unit --passWithNoTests",
    "test:integration": "jest --testPathPattern=tests/integration --passWithNoTests",
    "test:e2e": "cypress run",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "setup": "./scripts/setup.sh",
    "setup:env": "cp env.example .env && cp services/api-gateway/.env.example services/api-gateway/.env && cp services/security/.env.example services/security/.env && cp services/meeting-intelligence/.env.example services/meeting-intelligence/.env && cp services/workflow-automation/.env.example services/workflow-automation/.env",
    "db:migrate": "./scripts/db-migrate.sh",
    "db:seed": "PGPASSWORD=postgres psql -h localhost -p 5432 -U postgres -d qylon_dev -f database/seeds/01_subscription_plans.sql",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres redis mongodb && sleep 10 && npm run db:migrate && npm run db:seed",
    "deploy:staging": "node scripts/deployment/deploy-staging.js",
    "deploy:production": "node scripts/deployment/deploy-production.js",
    "infrastructure:plan": "cd infrastructure/terraform && terraform plan",
    "infrastructure:apply": "cd infrastructure/terraform && terraform apply",
    "infrastructure:destroy": "cd infrastructure/terraform && terraform destroy",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v && docker system prune -f",
    "health:check": "curl -f http://localhost:3000/health && curl -f http://localhost:3001/health && curl -f http://localhost:3004/health && curl -f http://localhost:3006/health",
    "test:performance:all": "./scripts/performance-test-enhanced.sh all",
    "test:coverage:monitor": "./scripts/coverage-monitor.sh",
    "test:coverage:trend": "./scripts/coverage-monitor.sh --trend",
    "test:coverage:upload": "./scripts/coverage-monitor.sh --upload",
    "security:scan": "./scripts/security-test.sh",
    "security:scan:npm": "./scripts/security-test.sh --npm-audit",
    "security:scan:snyk": "./scripts/security-test.sh --snyk",
    "security:scan:retire": "./scripts/security-test.sh --retire",
    "security:scan:audit-ci": "./scripts/security-test.sh --audit-ci",
    "security:scan:eslint": "./scripts/security-test.sh --eslint"
  },
  "keywords": [
    "ai",
    "automation",
    "meeting-intelligence",
    "workflow",
    "microservices",
    "supabase",
    "digitalocean"
  ],
  "author": "KD Squares Development Team",
  "license": "PROPRIETARY",
  "devDependencies": {
    "@types/cypress": "^0.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.6.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "concurrently": "^8.2.1",
    "cypress": "^13.3.0",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.23.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.3",
    "snyk": "^1.1248.0",
    "sonar-scanner": "^3.1.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "axios": "^1.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "redis": "^4.6.8",
    "uuid": "^9.0.0",
    "winston": "^3.10.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KD-Squares/KDS-Development.git"
  },
  "bugs": {
    "url": "https://github.com/KD-Squares/KDS-Development/issues"
  },
  "homepage": "https://github.com/KD-Squares/KDS-Development#readme"
}
