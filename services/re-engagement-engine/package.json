{
  "name": "re-engagement-engine",
  "version": "1.0.0",
  "description": "Qylon Re-engagement Engine - Handles email sequences, user behavior tracking, and conversion recovery",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:performance": "jest --config jest.performance.config.js",
    "lint": "npx eslint src/**/*.ts",
    "lint:fix": "npx eslint src/**/*.ts --fix",
    "format": "npx prettier --write src/**/*.ts",
    "format:check": "npx prettier --check src/**/*.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "joi": "^17.11.0",
    "axios": "^1.6.2",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3",
    "@supabase/supabase-js": "^2.38.4",
    "nodemailer": "^6.9.7",
    "@sendgrid/mail": "^8.1.0",
    "openai": "^4.20.1",
    "sentiment": "^5.0.2",
    "socket.io": "^4.7.4",
    "bull": "^4.12.2",
    "ioredis": "^5.3.2",
    "moment": "^2.29.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.4",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/uuid": "^9.0.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/jest": "^29.5.8",
    "@types/supertest": "^2.0.16",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "qylon",
    "re-engagement",
    "email-automation",
    "user-behavior",
    "conversion-recovery",
    "microservice",
    "typescript"
  ],
  "author": "Qylon Team",
  "license": "MIT"
}
